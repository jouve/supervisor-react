    "version": "0.2.0",
    "configurations": [
        {
            "name": "Supervisord",
            "type": "debugpy",
            "request": "launch",
            "module": "supervisor.supervisord",
            "args": ["-n"],
            "cwd": "${workspaceFolder}/tests",
